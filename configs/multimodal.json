{
  "additional_data": {
    "elevation": "/data/valhalla/elevation/"
  },
  "httpd": {
    "service": {
      "interrupt": "ipc:///tmp/interrupt",
      "listen": "tcp://*:8002",
      "loopback": "ipc:///tmp/loopback"
    }
  },
  "loki": {
    "actions": [
      "locate",
      "route",
      "height",
      "sources_to_targets",
      "optimized_route",
      "isochrone",
      "trace_route",
      "trace_attributes",
      "transit_available"
    ],
    "logging": {
      "color": true,
      "file_name": "path_to_some_file.log",
      "long_request": 100.0,
      "type": "std_out"
    },
    "service": {
      "proxy": "ipc:///tmp/loki"
    },
    "service_defaults": {
      "minimum_reachability": 100,
      "radius": 0
    },
    "use_connectivity": true
  },
  "meili": {
    "auto": {
      "search_radius": 50,
      "turn_penalty_factor": 200
    },
    "bicycle": {
      "turn_penalty_factor": 140
    },
    "customizable": [
      "mode",
      "search_radius",
      "turn_penalty_factor",
      "gps_accuracy",
      "sigma_z",
      "beta",
      "max_route_distance_factor",
      "max_route_time_factor"
    ],
    "default": {
      "beta": 3,
      "breakage_distance": 3000,
      "geometry": false,
      "gps_accuracy": 5.0,
      "interpolation_distance": 15,
      "max_route_distance_factor": 50,
      "max_route_time_factor": 50,
      "max_search_radius": 1000,
      "route": true,
      "search_radius": 500,
      "sigma_z": 4.07,
      "turn_penalty_factor": 0
    },
    "grid": {
      "cache_size": 10240,
      "size": 500
    },
    "logging": {
      "color": true,
      "file_name": "path_to_some_file.log",
      "type": "std_out"
    },
    "mode": "multimodal",
    "multimodal": {
      "turn_penalty_factor": 0
    },
    "pedestrian": {
      "search_radius": 500,
      "turn_penalty_factor": 10
    },
    "service": {
      "proxy": "ipc:///tmp/meili"
    },
    "verbose": true
  },
  "mjolnir": {
    "admin": "/data/valhalla/admins.sqlite",
    "hierarchy": true,
    "logging": {
      "color": true,
      "file_name": "path_to_some_file.log",
      "type": "std_out"
    },
    "max_cache_size": 10000000000,
    "shortcuts": true,
    "tile_dir": "/data/valhalla/tiles",
    "tile_extract": "/data/valhalla/tiles.tar",
    "timezone": "/data/valhalla/timezones.sqlite",
    "transit_dir": "/data/valhalla/transit"
  },
  "odin": {
    "logging": {
      "color": true,
      "file_name": "path_to_some_file.log",
      "type": "std_out"
    },
    "service": {
      "proxy": "ipc:///tmp/odin"
    }
  },
  "service_limits": {
    "auto": {
      "max_distance": 5000000.0,
      "max_locations": 20,
      "max_matrix_distance": 400000.0,
      "max_matrix_locations": 50
    },
    "auto_shorter": {
      "max_distance": 5000000.0,
      "max_locations": 20,
      "max_matrix_distance": 400000.0,
      "max_matrix_locations": 50
    },
    "bicycle": {
      "max_distance": 500000.0,
      "max_locations": 50,
      "max_matrix_distance": 200000.0,
      "max_matrix_locations": 50
    },
    "bus": {
      "max_distance": 5000000.0,
      "max_locations": 50000,
      "max_matrix_distance": 400000.0,
      "max_matrix_locations": 5000
    },
    "hov": {
      "max_distance": 5000000.0,
      "max_locations": 20,
      "max_matrix_distance": 400000.0,
      "max_matrix_locations": 50
    },
    "isochrone": {
      "max_contours": 4,
      "max_distance": 1500000.0,
      "max_locations": 1,
      "max_time": 6400
    },
    "max_avoid_locations": 50,
    "max_radius": 10000,
    "max_reachability": 1000000,
    "motor_scooter": {
      "max_distance": 500000.0,
      "max_locations": 50,
      "max_matrix_distance": 200000.0,
      "max_matrix_locations": 50
    },
    "multimodal": {
      "max_distance": 5000000.0,
      "max_locations": 50000,
      "max_matrix_distance": 0.0,
      "max_matrix_locations": 0
    },
    "pedestrian": {
      "max_distance": 250000.0,
      "max_locations": 50000,
      "max_matrix_distance": 200000.0,
      "max_matrix_locations": 5000,
      "max_transit_walking_distance": 3000,
      "min_transit_walking_distance": 1
    },
    "skadi": {
      "max_shape": 750000,
      "min_resample": 5.0
    },
    "trace": {
      "max_best_paths": 4,
      "max_best_paths_shape": 1000,
      "max_distance": 500000.0,
      "max_gps_accuracy": 100.0,
      "max_search_radius": 10000.0,
      "max_shape": 32000
    },
    "transit": {
      "max_distance": 5000000.0,
      "max_locations": 50000,
      "max_matrix_distance": 200000.0,
      "max_matrix_locations": 5000
    },
    "truck": {
      "max_distance": 5000000.0,
      "max_locations": 20,
      "max_matrix_distance": 400000.0,
      "max_matrix_locations": 50
    }
  },
  "thor": {
    "logging": {
      "color": true,
      "file_name": "path_to_some_file.log",
      "long_request": 1100.0,
      "type": "std_out"
    },
    "service": {
      "proxy": "ipc:///tmp/thor"
    },
    "source_to_target_algorithm": "select_optimal"
  }
}
